{
  "version": 2,
  "builds": [
    { "src": "src/server.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/sse", "dest": "src/server.py" },
    { "src": "/manifest.json", "dest": "src/server.py" },
    { "src": "/capabilities.json", "dest": "src/server.py" }
  ],
  "installCommand": "pip install --upgrade pip && pip install -r requirements.txt"
}
